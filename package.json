{
  "name": "ng2-adsense",
  "version": "5.1.3",
  "license": "MIT",
  "repository": "https://github.com/scttcper/ng2-adsense.git",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot",
    "build": "ng-packagr -p src/lib/package.json && node prepublish.js",
    "build:ghpages": "ng build --prod --baseHref='/ng2-adsense/' && ng build --prod --app 1 --output-hashing=false --no-progress",
    "build:client-and-server-bundles": "ng build --prod && ng build --prod --app 1 --output-hashing=false",
    "build:prerender": "npm run build:client-and-server-bundles && npm run webpack:server && npm run generate:prerender",
    "build:prerender-ghpages": "npm run build:ghpages && npm run webpack:server && npm run generate:prerender",
    "webpack:server": "webpack --config webpack.server.config.js --progress --colors",
    "generate:prerender": "cd dist && node prerender",
    "publish": "cd dist && npm publish",
    "bundlesize": "npx bundlesize",
    "test": "ng test --browsers=ChromeCI",
    "test:ci": "ng test --single-run --code-coverage --no-progress --browsers=ChromeCI",
    "lint": "ng lint --type-check -t stylish",
    "lint:fix": "ng lint --type-check --fix -t stylish",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/animations": "^5.2.1",
    "@angular/common": "^5.2.1",
    "@angular/compiler": "^5.2.1",
    "@angular/core": "^5.2.1",
    "@angular/forms": "^5.2.1",
    "@angular/http": "^5.2.1",
    "@angular/platform-browser": "^5.2.1",
    "@angular/platform-browser-dynamic": "^5.2.1",
    "@angular/platform-server": "^5.2.1",
    "@angular/router": "^5.2.1",
    "@nguniversal/express-engine": "^5.0.0-beta.5",
    "@nguniversal/module-map-ngfactory-loader": "^5.0.0-beta.5",
    "@ctrl/ngx-github-buttons": "^1.0.5",
    "bootstrap": "4.0.0",
    "core-js": "^2.5.3",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@angular/cli": "1.6.6",
    "@angular/compiler-cli": "^5.2.1",
    "@angular/language-service": "^5.2.1",
    "@types/jasmine": "~2.8.4",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^9.3.0",
    "codelyzer": "^4.1.0",
    "cpx": "^1.5.0",
    "jasmine-core": "^2.9.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^2.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.3",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "2.0.0-rc.11",
    "protractor": "^5.2.2",
    "ts-loader": "^3.2.0",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "2.6.x"
  },
  "bundlesize": [
    {
      "path": "./dist/bundles/ng2-adsense.umd.js",
      "maxSize": "2 kB"
    },
    {
      "path": "./dist/esm2015/ng2-adsense.js",
      "maxSize": "2 kB"
    },
    {
      "path": "./dist/esm5/ng2-adsense.js",
      "maxSize": "2 kB"
    }
  ]
}
