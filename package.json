    "publish": "cd dist && npm publish",
    "bundlesize": "npx bundlesize",
    "test": "ng test ---watch=false --browsers=ChromeCI",
    "test:watch": "ng test --browsers=ChromeCI",
    "test:ci": "ng test --watch=false --code-coverage --no-progress --browsers=ChromeCI",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "semantic-release": "cd dist && semantic-release"
  },
  "dependencies": {
    "@angular/animations": "8.0.0",
    "@angular/common": "8.0.0",
    "@angular/compiler": "8.0.0",
    "@angular/core": "8.0.0",
    "@angular/forms": "8.0.0",
    "@angular/platform-browser": "8.0.0",
    "@angular/platform-browser-dynamic": "8.0.0",
    "@angular/router": "8.0.0",
    "bootstrap": "4.3.1",
    "core-js": "3.1.3",
    "rxjs": "6.5.2",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.800.6",
    "@angular/cli": "8.0.6",
    "@angular/compiler-cli": "8.0.0",
    "@angular/language-service": "8.0.0",
    "@types/fs-extra": "7.0.0",
    "@types/jasmine": "3.3.13",
    "@types/node": "12.0.7",
    "codelyzer": "5.1.0",
    "del-cli": "2.0.0",
    "fs-extra": "8.0.1",
    "jasmine-core": "3.4.0",
    "karma": "4.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "2.0.0",
    "karma-coverage-istanbul-reporter": "2.0.5",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "karma-mocha-reporter": "2.2.5",
    "ng-packagr": "5.3.0",
    "semantic-release": "15.13.12",
    "ts-node": "8.2.0",
    "tsickle": "0.35.0",
    "tslint": "5.17.0",
    "typescript": "3.4.5"
  },
  "engines": {
    "npm": ">= 6",
    "node": ">= 8"
