{
  "name": "ng2-adsense",
  "version": "0.0.0-placeholder",
  "license": "MIT",
  "repository": "scttcper/ng2-adsense",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "prebuild": "rimraf dist",
    "build": "ts-node -O '{ \"module\": \"commonjs\" }' build.ts",
    "ghpages": "ng build --prod --no-progress --baseHref='/ng2-adsense/'",
    "publish": "cd dist && npm publish",
    "bundlesize": "npx bundlesize",
    "test": "ng test ---watch=false --browsers=ChromeCI",
    "test:watch": "ng test --browsers=ChromeCI",
    "test:ci": "ng test --watch=false --code-coverage --no-progress --browsers=ChromeCI",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "cd dist && semantic-release"
  },
  "dependencies": {
    "@angular/animations": "7.2.7",
    "@angular/common": "7.2.7",
    "@angular/compiler": "7.2.7",
    "@angular/core": "7.2.7",
    "@angular/forms": "7.2.7",
    "@angular/http": "7.2.7",
    "@angular/platform-browser": "7.2.7",
    "@angular/platform-browser-dynamic": "7.2.7",
    "@angular/router": "7.2.7",
    "@ctrl/ngx-github-buttons": "2.1.0",
    "bootstrap": "4.3.1",
    "core-js": "2.6.5",
    "rxjs": "6.4.0",
    "zone.js": "0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.13.4",
    "@angular/cli": "7.3.4",
    "@angular/compiler-cli": "7.2.7",
    "@angular/language-service": "7.2.7",
    "@types/fs-extra": "5.0.5",
    "@types/jasmine": "3.3.9",
    "@types/node": "11.10.4",
    "codelyzer": "4.5.0",
    "fs-extra": "7.0.1",
    "jasmine-core": "3.3.0",
    "karma": "4.0.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "2.0.0",
    "karma-coverage-istanbul-reporter": "2.0.5",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.0",
    "karma-mocha-reporter": "2.2.5",
    "ng-packagr": "4.7.1",
    "rimraf": "2.6.3",
    "semantic-release": "15.13.3",
    "travis-deploy-once": "5.0.11",
    "ts-node": "8.0.2",
    "tsickle": "0.34.3",
    "tslint": "5.13.1",
    "typescript": "3.2.4"
  },
  "engines": {
    "npm": ">= 6",
    "node": ">= 8"
  },
  "release": {
    "branch": "master"
  },
  "bundlesize": [
    {
      "path": "./dist/bundles/ng2-adsense.umd.min.js",
      "maxSize": "2 kB"
    }
  ]
}
