{
  "name": "ng2-adsense",
  "version": "5.1.0",
  "license": "MIT",
  "repository": "https://github.com/scttcper/ng2-adsense.git",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve --aot",
    "build": "rimraf dist && ts-node build.ts",
    "publish": "cd dist/packages-dist && npm publish",
    "bundlesize": "bundlesize",
    "test": "ng test --single-run --code-coverage",
    "ghpages": "ng build --prod --aot --build-optimizer --no-progress --baseHref='/ng2-adsense/'",
    "lint": "ng lint --type-check -t stylish",
    "lint:fix": "ng lint --type-check --fix -t stylish",
    "e2e": "ng e2e"
  },
  "dependencies": {
    "@angular/common": "^5.1.0",
    "@angular/compiler": "^5.1.0",
    "@angular/core": "^5.1.0",
    "@angular/platform-browser": "^5.1.0",
    "@angular/platform-browser-dynamic": "^5.1.0",
    "@angular/router": "^5.1.0",
    "@ctrl/ngx-github-buttons": "^1.0.0",
    "bootstrap": "^4.0.0-beta.2",
    "core-js": "^2.5.3",
    "rxjs": "^5.5.5",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "1.6.0",
    "@angular/compiler-cli": "^5.1.0",
    "@angular/language-service": "^5.1.0",
    "@types/jasmine": "~2.8.2",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^8.0.58",
    "bundlesize": "^0.15.3",
    "codelyzer": "^4.0.2",
    "copy": "^0.3.1",
    "fs-extra": "^5.0.0",
    "jasmine-core": "^2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.2.1",
    "rollup": "^0.52.1",
    "rollup-plugin-filesize": "^1.5.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "ts-node": "^4.0.1",
    "tslint": "^5.8.0",
    "typescript": "2.5.x"
  },
  "bundlesize": [
    {
      "path": "./dist/packages-dist/ng2-adsense.umd.js",
      "maxSize": "1.5 kB"
    },
    {
      "path": "./dist/packages-dist/ng2-adsense.es2015.js",
      "maxSize": "1.5 kB"
    },
    {
      "path": "./dist/packages-dist/ng2-adsense.es5.js",
      "maxSize": "1.5 kB"
    }
  ]
}
