{
  "name": "ng2-adsense",
  "version": "0.0.0-placeholder",
  "license": "MIT",
  "repository": "scttcper/ng2-adsense",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "del dist && ts-node -O '{ \"module\": \"commonjs\" }' build.ts",
